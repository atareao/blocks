{
    "$schema": "https://json.schemastore.org/block.json",
    "apiVersion": 2,
    "title": "AudioBlock",
    "name": "atareao/audioblock",
    "category": "embed",
    "icon": "universal-access-alt",
    "keywords": ["atareao", "audio"],
    "version": "1.0.0",
    "textdomain": "atareao-audioblock",
    "example": {},
    "attributes": {
        "src": {
            "type": "string",
            "source": "attribute",
            "selector": "audio",
            "attribute": "src",
            "default": ""
        },
        "preload": {
            "type": "string",
            "source": "attribute",
            "selector": "audio",
            "attribute": "preload",
            "default": "auto"
        },
        "style": {
            "type": "string",
            "source": "attribute",
            "selector": "audio",
            "attribute": "style",
            "default": "width: 100%"
        }
    },
    "editorScript": "file:./block.js",
    "style": "file:./style.css",
    "script": "file:./wavesurfer.js"
}

